<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="130.0" prefWidth="371.0" spacing="10" style="-fx-background-color: #424769;"
      xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="client.controllers.client.CancelOrderController">
    <children>
        <Button onAction="#goBack" text="Назад"/>
        <Label text="Выберите заказ, который хотите отменить" textFill="WHITE"/>
        <ComboBox fx:id="ordersComboBox" prefHeight="26.0" prefWidth="200.0"/>
        <Button onAction="#cancelOrder" text="Отменить заказ"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
